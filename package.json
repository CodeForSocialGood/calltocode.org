{
  "name": "calltocode.org",
  "version": "0.0.0",
  "description": "platform where college students can get development experience by working with nonprofits",
  "main": "index.js",
  "repository": "git@github.com:CodeForSocialGood/calltocode.org.git",
  "author": "c2c team",
  "private": true,
  "engines": {
    "node": "9.2.0"
  },
  "scripts": {
    "client:watch": "webpack --watch --hide-modules",
    "server:watch": "nodemon --watch server/ --ignore server/public/ --exec 'node server/'",
    "sync": "browser-sync start --proxy localhost:3000 --files server/**/* --port 3001 --ws --no-open --https",
    "prestart": "yarn db start && yarn email start && yarn clear-ports",
    "start": "npm-run-all --parallel client:watch server:watch sync",
    "stop": "yarn db stop && yarn email stop",
    "restart": "yarn stop && yarn start",
    "pretest": "yarn lint && yarn build",
    "test": "SILENT=true ava --verbose",
    "pree2e": "./e2e/pree2e.sh",
    "e2e": "nightwatch",
    "lint": "eslint --ignore-pattern server/public/ client/ server/",
    "build": "webpack --bail --display errors-only",
    "db": "./.setup/db/run.sh",
    "email": "./.setup/email/run.sh",
    "clear-ports": "./.setup/client/clear-ports.sh"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.1.4",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.7",
    "express": "^4.16.1",
    "express-jwt": "^5.3.0",
    "express-promise-router": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "hard-source-webpack-plugin": "^0.6.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.0.0",
    "jsonwebtoken": "^8.1.0",
    "material-ui": "^1.0.0-beta.29",
    "material-ui-icons": "^1.0.0-beta.17",
    "mongoose": "^5.0.0",
    "morgan": "^1.9.0",
    "node-sass": "^4.5.3",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-css-modules": "^4.7.1",
    "react-dom": "^16.0.0",
    "react-dropzone": "^4.2.5",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.2",
    "redux-form": "^7.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "rollbar": "^2.3.2",
    "sass-loader": "^6.0.6",
    "sendmail": "^1.2.0",
    "style-loader": "^0.20.0",
    "url-loader": "^1.0.0",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-visualizer-plugin": "^0.1.11",
    "file-loader": "^1.1.6"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "browser-sync": "^2.23.3",
    "chromedriver": "^2.34.1",
    "cucumber": "^4.0.0",
    "cucumber-pretty": "^1.3.2",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "geckodriver": "^1.10.0",
    "ignore-styles": "^5.0.1",
    "mongodb-memory-server": "^1.6.3",
    "nightwatch": "^0.9.19",
    "nightwatch-cucumber": "^9.0.0",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "phantomjs-prebuilt": "^2.1.16",
    "selenium-server": "^3.8.1",
    "sinon": "^4.0.0",
    "supertest": "^3.0.0"
  },
  "ava": {
    "files": [
      "client/**/*.test.js",
      "server/**/*.test.js"
    ],
    "require": [
      "babel-register",
      "ignore-styles"
    ],
    "babel": "inherit"
  }
}
